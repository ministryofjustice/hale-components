{
  "name": "ministryofjustice/wp-moj-components",
  "description": "A plugin to introduce global functions to a collection of WP sites",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "require": {
    "composer/installers": "*"
  },
  "require-dev": {
    "composer/composer": "*"
  },
  "autoload": {
    "psr-4": {
      "MOJComponents\\": "component/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
