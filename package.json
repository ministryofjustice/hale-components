{
    "name": "hale-components",
    "version": "1.0.0",
    "engines": {
        "node": "*",
        "npm": "*"
    },
    "description": "WP plugin hale-components",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development webpack --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production": "cross-env NODE_ENV=production webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ministryofjustice/hale-components.git"
    },
    "contributors": [
        "Ministry of Justice"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ministryofjustice/hale-components/issues"
    },
    "homepage": "https://github.com/ministryofjustice/hale-components/#readme",
    "author": "MoJ",
    "devDependencies": {
        "@babel/preset-env": "*",
        "copy-webpack-plugin": "*",
        "cross-env": "*",
        "laravel-mix": "*",
        "postcss": "^8.3.5",
        "resolve-url-loader": "*",
        "sass": "*",
        "sass-loader": "*"
    },
    "dependencies": {
        "@babel/core": "*",
        "govuk-frontend": "^4.6.0",
        "tippy.js": "*"
    }
}
